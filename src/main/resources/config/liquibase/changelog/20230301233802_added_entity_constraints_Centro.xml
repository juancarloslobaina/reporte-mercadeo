<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Centro.
    -->
    <changeSet id="20230301233802-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="ciudad_id"
                                 baseTableName="centro"
                                 constraintName="fk_centro__ciudad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ciudad"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="centro"
                                 constraintName="fk_centro__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
    </changeSet>
</databaseChangeLog>
